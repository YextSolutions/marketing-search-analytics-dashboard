{
  "$id": "marketingAnalytics_clicksByResultVerticalPosition",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Clicks by Result Vertical Position",
  "description": "Answers Clicks by Result Vertical Position",
  "query": {
    "metrics": [
      "yext/ANSWERS_CLICKS"
    ],
    "dimensions": [
      "yext/ANSWERS_RESULT_VERTICAL_POSITION"
    ],
    "filters": [
      {
        "filter": "yext/ANSWERS_CONFIGURATION_VERSION_LABEL",
        "in": [
          "PRODUCTION"
        ]
      }
    ]
  },
  "display": {
    "pie": {
      "showLabels": true
    }
  }
}